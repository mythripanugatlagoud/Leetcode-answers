class Solution {
public: 
    int subarraySum(vector<int>& nums, int k) {
        unordered_map<int,int>prefixcount;
        prefixcount[0] = 1;
        int sum = 0;
        int count = 0;

        for( int num : nums){
            sum += num;
            if (prefixcount.find(sum-k) != prefixcount.end())
            {
                count += prefixcount[sum-k];
            }
            prefixcount[sum]++;
        }
        return count;  
    }
};
